        <item name="colorPrimary">@color/light_blue_6</item>
        <item name="colorPrimaryVariant">@color/light_blue_2</item>
        <item name="colorOnPrimary">@color/dark_blue_4</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/dark_blue_2</item>
        <item name="colorSecondaryVariant">@color/dark_blue_1</item>
